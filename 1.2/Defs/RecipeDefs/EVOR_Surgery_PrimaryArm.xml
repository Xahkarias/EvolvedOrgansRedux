<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!--Normal ARM-->
  <RecipeDef ParentName="SurgeryFlesh">
    <defName>EVOR_Surgery_Primary_NormalArm</defName>
    <label>&lt;color=#00FFF7&gt;implant extradextrous arm&lt;/color&gt;</label>
    <description>Implant an extradextrous arm.</description>
    <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
    <jobString>Implanting extradextrous arm.</jobString>
    <workAmount>3000</workAmount>
    <skillRequirements>
      <Medicine>10</Medicine>
    </skillRequirements>
    <recipeUsers>
      <li>Human</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Medicine</li>
          </categories>
        </filter>
        <count>2</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>EVOR_Item_NormalArm</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Medicine</li>
      </categories>
      <thingDefs>
        <li>EVOR_Item_NormalArm</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Shoulder</li>
    </appliedOnFixedBodyParts>
    <addsHediff>EVOR_Hediff_Primary_NormalArm</addsHediff>
    <descriptionHyperlinks><ThingDef>EVOR_Item_NormalArm</ThingDef></descriptionHyperlinks>
  </RecipeDef>

