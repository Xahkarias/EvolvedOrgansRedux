<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RecipeDef>
		<defName>EVOR_Production_Mechanites</defName>
		<label>make nano mechanites</label>
		<description>Make some nano mechanites by reprograming parts of mechanoids.</description>
		<jobString>Making nano mechanites.</jobString>
		<workSpeedStat>ButcheryMechanoidSpeed</workSpeedStat>
		<effectWorking>ButcherMechanoid</effectWorking>
		<soundWorking>Recipe_ButcherCorpseMechanoid</soundWorking>
		<workAmount>250</workAmount>
		<recipeUsers>
			<li>TableMachining</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Corpses</li>
					</categories>
				</filter>
				<count>1</count>
      		</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentIndustrial</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>CorpsesMechanoid</li>
			</categories>
			<thingDefs>
				<li>ComponentIndustrial</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<EVOR_Resource_Mechanites>4</EVOR_Resource_Mechanites>
		</products>
		<researchPrerequisite>EVOR_Research_Start2</researchPrerequisite>
		<skillRequirements>
			<Crafting>10</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
	</RecipeDef>
	
