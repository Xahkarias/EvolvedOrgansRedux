          <value>
            <comps>
              <li Class="EBF.Hediffs.HediffCompProperties_MaxHPAdjust">
                <linearAdjustment>6</linearAdjustment>
              </li>
            </comps>
          </value>
			    <success>Always</success>
		    </li>

      </operations>
    </match>
  </Operation>
</Patch>