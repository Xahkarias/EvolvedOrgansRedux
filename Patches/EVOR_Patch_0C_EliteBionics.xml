<?xml version="1.0" encoding="utf-8" ?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <success>Always</success>
    <mods>
        <li>Elite Bionics Framework</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>

        <!--These cover all non-patched in arms-->
        <li Class="PatchOperationAdd">
          <xpath>/Defs/HediffDef[contains(defName, "EVOR_Hediff_Primary")]/comps</xpath>
          <value>
            <li Class="EBF.Hediffs.HediffCompProperties_MaxHPAdjust">
              <linearAdjustment>15</linearAdjustment>
            </li>
          </value>
			    <success>Always</success>
		    </li>
        <li Class="PatchOperationAdd">
          <xpath>/Defs/HediffDef[contains(defName, "EVOR_Hediff_Secondary")]/comps</xpath>
