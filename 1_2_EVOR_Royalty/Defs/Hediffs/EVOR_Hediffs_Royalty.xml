<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<HediffDef ParentName="ImplantHediffBase">
		<defName>EVOR_Hediff_Skull_Resonance</defName>
		<label>psychic resonance tissue</label>
		<defaultLabelColor>(.2,1,.9)</defaultLabelColor>
		<labelNoun>psychic resonance tissue</labelNoun>
		<spawnThingOnRemoved>EVOR_Item_Skull_Resonance</spawnThingOnRemoved>
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>1.0</partEfficiency>
			<betterThanNatural>true</betterThanNatural>
		</addedPartProps>
		<stages>
			<li>
				<statOffsets>
					<PsychicSensitivity>0.8</PsychicSensitivity>
				</statOffsets>
			</li>
		</stages>
		<description>By placing a layer of psychically sensitive tissue around the skull, the host becomes more attuned to psychic fields, allowing them to cast more powers before frying.</description>
		<descriptionHyperlinks><ThingDef>EVOR_Item_Skull_Resonance</ThingDef></descriptionHyperlinks>
	</HediffDef>


</Defs>